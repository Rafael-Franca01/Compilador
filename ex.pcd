cls Ponto {
  int x;
  int y;
};
cls Retangulo {
  Ponto cse; 
  Ponto cid; 
  int cor;
};

int main() {
  Retangulo r;
  int temp;

  
  r.cse.x = 10;
  r.cse.y = 20;
  r.cid.x = 99;
  r.cid.y = 88;
  r.cor = 255;

  
  prt(r.cse.x);
  prt(' ');
  prt(r.cse.y);
  prt('\n');

  
  prt(r.cid.x);
  prt(' ');
  prt(r.cid.y);
  prt('\n');
  
  
  prt(r.cor);
  prt('\n');

  
  r.cse.x = r.cse.x + 5; 
  
  prt(r.cse.x);
  prt('\n');

}