int main() {
    int A[2][3];
    int B[2][3];

    // Popula A e B
    A[0][0]=1; A[0][1]=2; A[0][2]=3;
    A[1][0]=4; A[1][1]=5; A[1][2]=6;
    B[0][0]=10; B[0][1]=10; B[0][2]=10;
    B[1][0]=10; B[1][1]=10; B[1][2]=10;

    // Testa Soma
    int C[2][3];
    C = A + B;
    prt("Soma C[1][2]: ");
    prt(C[1][2]); // 6 + 10 = 16
    prt("\n");

    // Testa Multiplicação
    int T[3][2]; // Matriz Transposta para permitir multiplicação
    T[0][0]=1; T[0][1]=1;
    T[1][0]=1; T[1][1]=1;
    T[2][0]=1; T[2][1]=1;

    int M[2][2];
    M = A * T;
    prt("Multiplicacao M[0][0]: ");
    prt(M[0][0]); // (1*1 + 2*1 + 3*1) = 6
    prt("\n");

    // int D[1][1];
    // D = A * C; // DESCOMENTE ESTA LINHA PARA TESTAR O ERRO DE COMPILAÇÃO
}